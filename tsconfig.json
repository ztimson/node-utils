{
	"include": ["src"],
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"lib": ["ESNext"],
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"declaration": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true
	}
}
